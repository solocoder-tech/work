<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black">#ff000000</color>
    <color name="blue">#ff270cf7</color>
    <color name="drap_bar_word_color">#ff3e7ec8</color>
    <color name="gray">#ff888888</color>
    <color name="lucency">#00000000</color>
    <color name="update_blue">#ff3e81c8</color>
    <color name="white">#ffffffff</color>
    <declare-styleable name="gifView"><attr format="integer" name="src"/><attr format="integer" name="delay"/><attr format="boolean" name="stop"/></declare-styleable>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <string name="action_settings">Settings</string>
    <string name="app_name">WIFIDemo</string>
    <string name="hello_world">Hello world!</string>
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="loading_small" parent="@android:style/Widget.ProgressBar.Small">
        <item name="android:indeterminateDrawable">@anim/loading_anim</item>
        <item name="android:layout_width">32dip</item>
        <item name="android:layout_height">32dip</item>
        <item name="android:layout_gravity">center</item>
    </style>
    <style name="textview_large">
        <item name="android:textSize">22.0sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
</resources>